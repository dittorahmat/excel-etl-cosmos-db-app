{
  "name": "excel-to-cosmos-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "server"
  ],
  "scripts": {
    "dev:client": "vite --port 3000 --force",
    "dev:server": "cd server && PORT=3001 npm run dev",
    "dev": "concurrently -n 'CLIENT,SERVER' -c 'bgBlue.bold,bgGreen.bold' \"npm run dev:client\" \"npm run dev:server\"",
    "prebuild:client": "node scripts/generate-config.js",
    "build:client": "vite build",
    "postbuild:client": "node scripts/fix-css-name.cjs",
    "preview": "vite preview",
    "preview:full": "npm run build && concurrently \"npm run start:server\" \"npm run preview\"",
    "build:server": "tsc -b server/tsconfig.server.build.json",
    "start:server": "npx dotenv -e server/.env -- node server/dist/src/server.js",
    "build": "rm -rf node_modules/.vite && rm -rf dist && rm -rf server/dist && npm run build:client && npm run build:server",
    "build:client-only": "rm -rf dist && npm run build:client",
    "clean": "rm -rf common/dist server/dist",
    "start": "node server/dist/src/server.js",
    "test": "npm run test:server && npm run test:client",
    "test:watch": "vitest --config vitest.optimized.config.ts watch",
    "test:coverage": "npm run test:server:coverage && npm run test:client:coverage",
    "test:server": "NODE_OPTIONS=--max-old-space-size=2048 vitest run -c vitest.server.config.ts server/test --exclude='**/backup_js_tests/**'",
    "test:server:watch": "NODE_OPTIONS=--max-old-space-size=2048 vitest watch -c vitest.server.config.ts server/test",
    "test:server:coverage": "NODE_OPTIONS=--max-old-space-size=2048 vitest run --coverage -c vitest.server.config.ts server/test",
    "test:client": "NODE_OPTIONS=\"--max-old-space-size=2048\" vitest run -c vitest.optimized.config.ts --passWithNoTests",
    "test:client:watch": "vitest watch -c vitest.optimized.config.ts src --dir src --passWithNoTests",
    "test:client:coverage": "vitest run --coverage -c vitest.optimized.config.ts --passWithNoTests",
    "test:all": "vitest run -c vitest.optimized.config.ts src --dir src --passWithNoTests && NODE_OPTIONS=--max-old-space-size=2048 vitest run -c vitest.server.config.ts server/test",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern '**/node_modules/*' --ignore-pattern '**/dist/*' --ignore-pattern '**/coverage/*'",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc -p tsconfig.typecheck.json --noEmit",
    "dev:swa": "swa start http://localhost:3000 --run \"npm run dev\" --swa-config-location ./staticwebapp.mock.config.json",
    "postbuild:client:cdn": "node scripts/add-cdn-scripts.cjs"
  },
  "dependencies": {
    "@azure/cosmos": "^4.4.1",
    "@azure/identity": "^4.1.0",
    "@azure/msal-node": "^3.6.0",
    "@azure/msal-react": "^2.0.0",
    "@azure/storage-blob": "^12.27.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.14",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@upstash/context7-mcp": "^1.0.12",
    "class-variance-authority": "^0.7.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.1",
    "open": "^10.1.2",
    "react": "^18.2.0",
    "react-day-picker": "^9.8.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "uuid": "^9.0.1",
    "vite-plugin-html": "^3.2.2"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.1",
    "@eslint/js": "^9.9.0",
    "@faker-js/faker": "^9.3.0",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.1",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^13.24.0",
    "jsdom": "^24.1.3",
    "lucide-react": "^0.294.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.43.1",
    "tinyexec": "^1.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.1",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.5.1"
}
