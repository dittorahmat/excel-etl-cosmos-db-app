{
  "name": "excel-to-cosmos-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "server"
  ],
  "scripts": {
    "dev:client": "vite --port 3000 --force",
    "dev:server": "cd server && PORT=3001 npm run dev",
    "dev": "npx concurrently -n 'CLIENT,SERVER' -c 'bgBlue.bold,bgGreen.bold' \"npm run dev:client\" \"npm run dev:server\"",
    "prebuild:client": "node scripts/generate-config.js",
    "build:client": "vite build",
    "postbuild:client": "node scripts/copy-dist.js",
    "preview": "vite preview",
    "preview:full": "npm run build && concurrently \"npm run start:server\" \"npm run preview\"",
    "build:server": "tsc -b server/tsconfig.server.build.json",
    "start:server": "node server/dist/server/src/server.js",
    "build": "rm -rf node_modules/.vite && rm -rf dist && rm -rf server/dist && npm run build:client && npm run build:server",
    "postbuild": "cp .env server/dist/ 2>/dev/null || echo 'No .env file to copy'",
    "build:client-only": "rm -rf dist && npm run build:client",
    "clean": "rm -rf common/dist server/dist",
    "start": "node server/dist/server/src/server.js",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:server": "vitest run -c vitest.server.config.ts",
    "test:server:watch": "vitest watch -c vitest.server.config.ts",
    "test:server:coverage": "NODE_OPTIONS=--max-old-space-size=2048 vitest run --coverage -c vitest.server.config.ts",
    "test:client": "vitest run -c vitest.client.config.ts",
    "test:client:watch": "vitest watch -c vitest.client.config.ts",
    "test:client:coverage": "vitest run --coverage -c vitest.client.config.ts --passWithNoTests",
    "test:contracts": "cd contract-tests && npm test",
    "test:mutation": "cd mutation-tests && npm test",
    "test:performance": "cd performance-tests && npm test",
    "test:all": "vitest run",
    "lint": "eslint src/**/*.{ts,tsx} server/src/**/*.{ts,tsx} --ignore-pattern \"**/*.d.ts\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc -p tsconfig.typecheck.json --noEmit",
    "dev:swa": "swa start http://localhost:3000 --run \"npm run dev\" --swa-config-location ./staticwebapp.mock.config.json",
    "postbuild:client:cdn": "node scripts/add-cdn-scripts.cjs",
    "test:shared": "vitest run --dir tests/shared",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "@azure/cosmos": "^4.5.0",
    "@azure/identity": "^4.11.1",
    "@azure/msal-node": "^3.7.2",
    "@azure/msal-react": "^2.2.0",
    "@azure/storage-blob": "^12.28.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@upstash/context7-mcp": "^1.0.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "jwks-rsa": "^3.2.0",
    "lucide-react": "^0.294.0",
    "multer": "^2.0.1",
    "next-themes": "^0.4.6",
    "open": "^10.1.2",
    "plugin-react": "^0.0.1-security",
    "react": "^18.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.8.2",
    "recharts": "^2.15.4",
    "tailwind-merge": "^2.6.0",
    "uuid": "^9.0.1",
    "vite-plugin-html": "^3.2.2"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.6",
    "@eslint/js": "^9.35.0",
    "@faker-js/faker": "^9.3.0",
    "@rolldown/pluginutils": "^1.0.0-beta.33",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.19",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.25",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fs-extra": "^11.3.1",
    "globals": "^13.24.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "terser": "^5.43.1",
    "tinyexec": "^1.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.2.4",
    "vitest-sonar-reporter": "^2.0.1",
    "xlsx": "^0.18.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.5.1"
}
